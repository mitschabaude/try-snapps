{
  "name": "try-snapps",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [],
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "testw": "jest --watch",
    "coverage": "jest --coverage",
